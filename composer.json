{
    "name": "zul3s/enum-php",
    "description": "Advanced Php Enum Type",
    "type": "library",
    "license": "MIT",
    "keywords": ["enum"],
    "authors": [
        {
            "name": "Julien Zirnheld",
            "email": "julienzirnheld@gmail.com",
            "homepage": "http://julienzirnheld.fr"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*",
        "phpbench/phpbench": "@dev",
        "phpro/grumphp": "^0.11.6",
        "squizlabs/php_codesniffer": "^3.0",
        "sebastian/phpcpd": "^3.0",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "sensiolabs/security-checker": "^4.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
          "Zul3s\\EnumPhp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zul3s\\Tests\\Enum\\": "tests/"
        }
    }
